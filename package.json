  "author": "jrainliu <jrainliu@tencent.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-updater": "^5.0.5"
  }
}